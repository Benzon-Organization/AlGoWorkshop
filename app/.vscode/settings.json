{    
    "al.enableCodeAnalysis": true,    
    "al.codeAnalyzers": [
        "${CodeCop}",
        "${UICop}",
        "${AppSourceCop}"
    ],  
    "al.ruleSetPath": "./.vscode/bsd.ruleset.json",
    

    "CRS.FileNamePattern": "<ObjectNameShort>.<ObjectTypeShortPascalCase>.al",
    "CRS.FileNamePatternExtensions": "<ObjectNameShort>.<ObjectTypeShortPascalCase>.al",
    "CRS.ObjectNamePrefix": "BSD",
    "CRS.OnSaveAlFileAction": "Rename",
    "CRS.RenameWithGit": true,
    "CRS.RemovePrefixFromFilename": true,
    "CRS.DisableCRSSnippets": false,
    "CRS.DisableDefaultAlSnippets": true,    
    
    "alOutline.completionProviders": ["VariableNamesWithType"],

    "NAB.EnableTelemetry": false,  
}